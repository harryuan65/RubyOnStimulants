$(".section-content-container").removeClass('jc');
$(".section-content-container").addClass("fade-out");
$(".section-content-container").on('animationend', function(){
  if (event.animationName !== 'fade-out') {
    return;
  }
  $(".section-content-container").html("<%= j render("list")%>");
  $(".list").ready(function(){
    $(".section-content-container").removeClass("fade-out");
    $(".section-content-container").addClass("fade-in");
  })
});



