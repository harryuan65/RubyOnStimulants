<% if @remove_id %>
  delete currentData("<%=@remove_id%>")
<% end %>
$("#list-container").addClass("fade-out");
$("#list-container").on('animationend', function(){
  if (event.animationName !== 'fade-out') {
    return;
  }
  $("#list-container").html("<%= j render("list")%>");
  $(".list").ready(function(){
    $("#list-container").removeClass("fade-out");
    $("#list-container").addClass("fade-in");
  })
});



