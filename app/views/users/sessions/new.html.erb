<%= render('shared/header',title:"Welcome") %>

<div id="container-login">
  <%- if devise_mapping.omniauthable? %>
    <%- resource_class.omniauth_providers.each do |provider| %>
      <%= link_to "#{image_tag("#{OmniAuth::Utils.camelize(provider)}.png",class:"icon icon-#{OmniAuth::Utils.camelize(provider)}")} #{OmniAuth::Utils.camelize(provider)}".html_safe, omniauth_authorize_path(resource_name, provider),class:"login-option" %><br />
    <% end %>
  <% end %>
</div>